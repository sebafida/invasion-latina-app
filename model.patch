-------------------+            customer_email=current_user["email"]
-------------------+        )
-------------------+        
-------------------+        if not payment_result["success"]:
-------------------+            raise HTTPException(status_code=400, detail="Payment failed")
-------------------+        
-------------------+    except Exception as e:
